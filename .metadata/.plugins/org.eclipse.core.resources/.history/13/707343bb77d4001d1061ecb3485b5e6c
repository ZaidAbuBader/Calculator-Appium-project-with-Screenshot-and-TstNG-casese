
public class MyClass2 {

	public static void main(String[] args) {

		String[] cars = { "Volvo", "BMW", "Ford", "Mazda" };
		for (int i = 0; i < 2; i++);
		{
			for (int n = 0; n < cars.length; n++) {
				System.out.println(cars[n]);
			                                      }
		}
	                                        }
                      }
